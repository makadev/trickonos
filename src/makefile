
wporelease:
	$(FPC) -FW../$(OBJ_PATH)/wpo1 -OWall $(FILEFLAGS) $(FPCFLAGS) $(RELEASE_FLAGS) $(PROJECTFILE)
	$(FPC) -Fw../$(OBJ_PATH)/wpo1 -Owall -FW../$(OBJ_PATH)/wpo2 -OWall $(FILEFLAGS) $(FPCFLAGS) $(RELEASE_FLAGS) $(PROJECTFILE)
	$(FPC) -Fw../$(OBJ_PATH)/wpo2 -Owall $(FILEFLAGS) $(FPCFLAGS) $(RELEASE_FLAGS) $(PROJECTFILE)

release:
	$(FPC) $(FILEFLAGS) $(FPCFLAGS) $(RELEASE_FLAGS) $(PROJECTFILE)

debug:
	$(FPC) $(FILEFLAGS) $(FPCFLAGS) $(DEBUG_FLAGS) $(PROJECTFILE)

memdebug:
	$(FPC) $(FILEFLAGS) $(FPCFLAGS) $(MEMDEBUG_FLAGS) $(PROJECTFILE)

vgdebug:
	$(FPC) $(FILEFLAGS) $(FPCFLAGS) $(VGDEBUG_FLAGS) $(PROJECTFILE)

vgrelease:
	$(FPC) $(FILEFLAGS) $(FPCFLAGS) $(VGRELEASE_FLAGS) $(PROJECTFILE)
